#include <stdio.h>

int main()
{
	int i,b,a;
	
	int tab[1000];
	
	 a=1;
	 i=1;
	 while (a>0)
	 {
		 printf ("podaj liczbe ");
		 scanf("%i", &a);
		 tab[i]=a;
		 b=b+1;
		 i=i+1;
		} 
	  
	for (i=(i-1);i>0;i--)
	 {
		 printf("%i ", tab[i]);
	    
	} 
	printf (" \n");
  printf ("koniec programu");
	return 0;
}
