#include <stdio.h> 
int qq(int n, int k) 
{ 
  int i; 
  int x=0; 
  
  if (n==0) 
  { 
  return 0; 
  }
    else 
    { 
      for (i=0; i<k; i++) 
      { 
      x=x+(i*i); 
      } 
    return qq(n-1, k)+x; 
    } 
    
} 
  
  
int main() 
{ 
int n, k; 
printf("n: "); 
scanf("%i", &n); 
printf("k: "); 
scanf("%i", &k); i
f (k<0 || n<0) 
{ 
printf("k oraz n muszą być liczbami naturalnymi\n");
return 0; 
} 
printf("%i\n", qq(n,k)); return 0; 
}
